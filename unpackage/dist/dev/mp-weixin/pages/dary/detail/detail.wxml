<view class="content-bg"><view class="user-message"><view class="dary-title">{{''+from.title}}</view><view class="dary-detail"><text>{{''+from.content}}</text></view><view class="images flex-row"><block wx:for="{{from.imaging}}" wx:for-item="item" wx:for-index="i" wx:key="i"><view><image style="margin:10rpx;border:3rpx solid #ccc;width:200rpx;height:200rpx;" src="{{item}}" data-event-opts="{{[['tap',[['previewImag',[i]]]]]}}" bindtap="__e"></image></view></block></view><view class="locationMessage"><block wx:if="{{from.locations}}"><text style="font-size:35rpx;color:blue;">@</text></block><text>{{from.locations}}</text></view><block wx:if="{{from.video}}"><view class="videos"><video style="width:100%;height:300rpx;" src="{{from.video}}"></video></view></block><block wx:if="{{from.audio}}"><view class="audios"><audio style="width:100%;" src="{{from.audio}}" controls="{{true}}" name="{{from.audioName}}" poster="{{audio_bg}}"></audio></view></block><block wx:if="{{from.darydate}}"><view class="bottom flex-row"><view class="dary-date">{{from.darydate.year+"年"+from.darydate.month+"月"+from.darydate.day+"日"}}</view></view></block></view></view>